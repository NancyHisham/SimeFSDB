            "Email", "PhoneNumber", "PassportId", "FlightId", "SeatNumber", "luggageWeight", "ReservationId"],
            "primary_key"  : "ReservationId",
            "Index_keys" : ["First_name", "Last_name", "Gender"],
            "Consistency" : "Eventual"
        },
        {
            "name" : "FlightInfo",
            "columns" : ["PlaneId", "AirLineName", "From", "To", "DepartureTime", "ArrivalTime", "FlightId"],
            "primary_key"  : "FlightId",
            "Index_keys" : ["From", "To", "DepartureTime", "ArrivalTime"],
            "Consistency" : "Strong"
        },
        {
            "name" : "PlaneDetails",
            "columns" : [ "SeatsDistribution", "OccupiedSeats", "PlaneId", "last name"],
            "primary_key"  : "PlaneId",
            "Index_keys" : ["last name"],
            "Consistency" : "Eventual"
        },
        {
            "name" : "FlightSeats",
            "columns" : [ "ReservationId", "FlightId_SeatNumber", "Last_name"],
            "primary_key"  : "FlightId_SeatNumber",
